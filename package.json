{
  "name": "token-exporter-ds",
  "version": "1.0.0",
  "description": "![Token Exporter Hero Image](https://res.cloudinary.com/dbmvymdp1/image/upload/v1752106527/Figma_01_sk7ne5.png)",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "npm run dev",
    "sync": "npm run figma-check && npm run template-check && bash scripts/sync.sh",
    "lint:js": "ESLINT_USE_FLAT_CONFIG=false eslint src/code.js",
    "figma-check": "node scripts/figma-compat-check.js",
    "template-check": "node scripts/template-validator.js",
    "template-validate": "node scripts/template-validator.js",
    "test": "npm run figma-check && npm run lint:js && npm run template-check",
    "dev": "nodemon --watch scripts/ --watch docs/ --watch src/ --ext \"sh,html,css\" --ignore src/ui.html --ignore docs/design-system-guide.html --exec \"npm run sync\"",
    "build": "npm run sync",
    "sync:skip-checks": "bash scripts/sync.sh",
    "format": "stylelint docs/design-system.css --fix",
    "clean": "rm -f src/ui.html.backup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n8mills-UI/token-exporter-ds.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/n8mills-UI/token-exporter-ds/issues"
  },
  "homepage": "https://github.com/n8mills-UI/token-exporter-ds#readme",
  "devDependencies": {
    "eslint": "^9.31.0",
    "nodemon": "^3.1.10",
    "stylelint": "^16.21.1",
    "stylelint-config-standard": "^38.0.0"
  }
}